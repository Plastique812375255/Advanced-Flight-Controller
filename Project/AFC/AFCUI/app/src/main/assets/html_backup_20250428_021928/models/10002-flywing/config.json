  "defaults": {
    "widgets": [
      {"id": "medium", "source": "common", "position": {"x": 0, "y": 0}, "size": {"w": 2, "h": 1}},
      {"id": "10002-dashboard", "source": "model", "position": {"x": 2, "y": 0}, "size": {"w": 2, "h": 1}},
      {"id": "model-name", "source": "common", "position": {"x": 0, "y": 1}, "size": {"w": 1, "h": 1}},
      {"id": "small", "source": "common", "position": {"x": 1, "y": 1}, "size": {"w": 1, "h": 1}},
      {"id": "model-image", "source": "common", "position": {"x": 0, "y": 2}, "size": {"w": 2, "h": 2}},
      {"id": "large", "source": "common", "position": {"x": 2, "y": 1}, "size": {"w": 2, "h": 2}}
    ]
  }
} 